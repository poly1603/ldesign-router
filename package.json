{
  "name": "@ldesign/router",
  "version": "1.0.0",
  "private": true,
  "description": "Multi-framework router library workspace for ldesign",
  "keywords": [
    "router",
    "navigation",
    "spa",
    "typescript",
    "performance",
    "vue",
    "react",
    "svelte",
    "solid",
    "angular",
    "multi-framework"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./ab-testing": {
      "types": "./es/ab-testing/index.d.ts",
      "import": "./es/ab-testing/index.js",
      "require": "./lib/ab-testing/index.cjs"
    },
    "./ab-testing/*": {
      "types": "./es/ab-testing/*.d.ts",
      "import": "./es/ab-testing/*.js",
      "require": "./lib/ab-testing/*.cjs"
    },
    "./analytics/*": {
      "types": "./es/analytics/*.d.ts",
      "import": "./es/analytics/*.js",
      "require": "./lib/analytics/*.cjs"
    },
    "./components": {
      "types": "./es/components/index.d.ts",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.cjs"
    },
    "./components/*": {
      "types": "./es/components/*.d.ts",
      "import": "./es/components/*.js",
      "require": "./lib/components/*.cjs"
    },
    "./composables": {
      "types": "./es/composables/index.d.ts",
      "import": "./es/composables/index.js",
      "require": "./lib/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    },
    "./config/*": {
      "types": "./es/config/*.d.ts",
      "import": "./es/config/*.js",
      "require": "./lib/config/*.cjs"
    },
    "./core": {
      "types": "./es/core/index.d.ts",
      "import": "./es/core/index.js",
      "require": "./lib/core/index.cjs"
    },
    "./core/*": {
      "types": "./es/core/*.d.ts",
      "import": "./es/core/*.js",
      "require": "./lib/core/*.cjs"
    },
    "./data-fetching": {
      "types": "./es/data-fetching/index.d.ts",
      "import": "./es/data-fetching/index.js",
      "require": "./lib/data-fetching/index.cjs"
    },
    "./data-fetching/*": {
      "types": "./es/data-fetching/*.d.ts",
      "import": "./es/data-fetching/*.js",
      "require": "./lib/data-fetching/*.cjs"
    },
    "./debug/*": {
      "types": "./es/debug/*.d.ts",
      "import": "./es/debug/*.js",
      "require": "./lib/debug/*.cjs"
    },
    "./device": {
      "types": "./es/device/index.d.ts",
      "import": "./es/device/index.js",
      "require": "./lib/device/index.cjs"
    },
    "./device/*": {
      "types": "./es/device/*.d.ts",
      "import": "./es/device/*.js",
      "require": "./lib/device/*.cjs"
    },
    "./engine": {
      "types": "./es/engine/index.d.ts",
      "import": "./es/engine/index.js",
      "require": "./lib/engine/index.cjs"
    },
    "./engine/*": {
      "types": "./es/engine/*.d.ts",
      "import": "./es/engine/*.js",
      "require": "./lib/engine/*.cjs"
    },
    "./features/*": {
      "types": "./es/features/*.d.ts",
      "import": "./es/features/*.js",
      "require": "./lib/features/*.cjs"
    },
    "./guards": {
      "types": "./es/guards/index.d.ts",
      "import": "./es/guards/index.js",
      "require": "./lib/guards/index.cjs"
    },
    "./guards/*": {
      "types": "./es/guards/*.d.ts",
      "import": "./es/guards/*.js",
      "require": "./lib/guards/*.cjs"
    },
    "./managers/*": {
      "types": "./es/managers/*.d.ts",
      "import": "./es/managers/*.js",
      "require": "./lib/managers/*.cjs"
    },
    "./micro-frontend": {
      "types": "./es/micro-frontend/index.d.ts",
      "import": "./es/micro-frontend/index.js",
      "require": "./lib/micro-frontend/index.cjs"
    },
    "./micro-frontend/*": {
      "types": "./es/micro-frontend/*.d.ts",
      "import": "./es/micro-frontend/*.js",
      "require": "./lib/micro-frontend/*.cjs"
    },
    "./middleware": {
      "types": "./es/middleware/index.d.ts",
      "import": "./es/middleware/index.js",
      "require": "./lib/middleware/index.cjs"
    },
    "./middleware/*": {
      "types": "./es/middleware/*.d.ts",
      "import": "./es/middleware/*.js",
      "require": "./lib/middleware/*.cjs"
    },
    "./plugins": {
      "types": "./es/plugins/index.d.ts",
      "import": "./es/plugins/index.js",
      "require": "./lib/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    },
    "./state/*": {
      "types": "./es/state/*.d.ts",
      "import": "./es/state/*.js",
      "require": "./lib/state/*.cjs"
    },
    "./type-generator": {
      "types": "./es/type-generator/index.d.ts",
      "import": "./es/type-generator/index.js",
      "require": "./lib/type-generator/index.cjs"
    },
    "./type-generator/*": {
      "types": "./es/type-generator/*.d.ts",
      "import": "./es/type-generator/*.js",
      "require": "./lib/type-generator/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "pnpm -r --filter=@ldesign/router-* run build",
    "build:core": "pnpm --filter=@ldesign/router-core run build",
    "build:vue": "pnpm --filter=@ldesign/router-vue run build",
    "build:react": "pnpm --filter=@ldesign/router-react run build",
    "build:svelte": "pnpm --filter=@ldesign/router-svelte run build",
    "build:solid": "pnpm --filter=@ldesign/router-solid run build",
    "build:angular": "pnpm --filter=@ldesign/router-angular run build",
    "build:watch": "pnpm -r --filter=@ldesign/router-* run build:watch",
    "type-check": "pnpm -r run type-check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "pnpm -r run clean && rimraf es lib dist",
    "test": "pnpm -r run test",
    "dev": "pnpm -r --parallel run dev",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5",
    "@antfu/eslint-config": "^6.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "size-limit": [
    {
      "name": "UMD Bundle",
      "path": "dist/index.js",
      "limit": "45 KB"
    },
    {
      "name": "UMD Bundle (minified)",
      "path": "dist/index.min.js",
      "limit": "20 KB"
    },
    {
      "name": "ES Module",
      "path": "esm/index.js",
      "limit": "40 KB",
      "import": "{ createRouter }"
    },
    {
      "name": "CommonJS",
      "path": "cjs/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependenciesMeta": {}
}