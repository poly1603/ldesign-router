{
  "name": "@ldesign/router",
  "version": "1.0.0",
  "description": "A modern, high-performance Vue router library with TypeScript support, independent of vue-router",
  "keywords": [
    "vue",
    "router",
    "navigation",
    "spa",
    "typescript",
    "performance",
    "modern",
    "independent"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
