    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    },
    "./state/*": {
      "types": "./es/state/*.d.ts",
      "import": "./es/state/*.js",
      "require": "./lib/state/*.cjs"
    },
    "./type-generator": {
      "types": "./es/type-generator/index.d.ts",
      "import": "./es/type-generator/index.js",
      "require": "./lib/type-generator/index.cjs"
    },
    "./type-generator/*": {
      "types": "./es/type-generator/*.d.ts",
      "import": "./es/type-generator/*.js",
      "require": "./lib/type-generator/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "benchmark": "node scripts/performance-benchmark.js",
    "benchmark:regression": "node scripts/performance-regression.js",
    "benchmark:comparison": "node scripts/performance-comparison.js",
    "clean": "rimraf es lib dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "build:check": "pnpm run build && node ../../tools/scripts/build/bundle-validator.js",
    "build:analyze": "pnpm run build && node ../../tools/scripts/build/bundle-analyzer.js",
    "build:validate": "pnpm run build && node ../../tools/scripts/build/validate-build.js",
    "build:browser-test": "pnpm run build && node ../../tools/scripts/build/browser-tester.js",
    "validate": "pnpm run type-check && pnpm run test:run && pnpm run build",
    "validate:quick": "pnpm run type-check",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "commit-msg": "commitlint --edit",
    "commit:interactive": "node scripts/commit.js",
    "validate:full": "node scripts/validate.js",
    "validate:e2e": "node scripts/validate.js --e2e",
    "build:dev": "node scripts/build.js --dev",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ldesign/device": "workspace:*",
    "mitt": "^3.0.1",
    "nanoid": "^5.0.9"
  },
  "peerDependencies": {
    "@ldesign/device": "^1.0.0",
    "@ldesign/engine": "^1.0.0",
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5",
    "@antfu/eslint-config": "^6.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "size-limit": [
    {
      "name": "UMD Bundle",
      "path": "dist/index.js",
      "limit": "45 KB"
    },
    {
      "name": "UMD Bundle (minified)",
      "path": "dist/index.min.js",
      "limit": "20 KB"
    },
    {
      "name": "ES Module",
      "path": "esm/index.js",
      "limit": "40 KB",
      "import": "{ createRouter }"
    },
    {
      "name": "CommonJS",
      "path": "cjs/index.js",
      "limit": "40 KB"
    }
  ],
  "peerDependenciesMeta": {
    "@ldesign/device": {
      "optional": false
    },
    "@ldesign/engine": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  }
}