{
  "name": "@ldesign/router",
  "version": "1.0.0",
  "description": "Multi-framework router library workspace for ldesign",
  "keywords": [
    "router",
    "navigation",
    "spa",
    "typescript",
    "performance",
    "vue",
    "react",
    "svelte",
    "solid",
    "angular",
    "multi-framework"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": "./packages/core/es/index.js",
    "./core": "./packages/core/es/index.js",
    "./vue": "./packages/vue/es/index.js",
    "./react": "./packages/react/es/index.js",
    "./solid": "./packages/solid/es/index.js",
    "./svelte": "./packages/svelte/es/index.js",
    "./angular": "./packages/angular/es/index.js",
    "./lit": "./packages/lit/es/index.js",
    "./preact": "./packages/preact/es/index.js",
    "./qwik": "./packages/qwik/es/index.js",
    "./vue2": "./packages/vue2/es/index.js"
  },
  "main": "./packages/core/lib/index.cjs",
  "module": "./packages/core/es/index.js",
  "types": "./packages/core/es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs,dts",
    "build:core": "pnpm --filter=@ldesign/router-core run build",
    "build:vue": "pnpm --filter=@ldesign/router-vue run build",
    "build:react": "pnpm --filter=@ldesign/router-react run build",
    "build:svelte": "pnpm --filter=@ldesign/router-svelte run build",
    "build:solid": "pnpm --filter=@ldesign/router-solid run build",
    "build:angular": "pnpm --filter=@ldesign/router-angular run build",
    "build:watch": "pnpm -r --filter=@ldesign/router-* run build:watch",
    "type-check": "pnpm -r run type-check",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "pnpm -r run clean && rimraf es lib dist",
    "test": "pnpm -r run test",
    "dev": "ldesign-builder build -f esm,cjs,dts --watch",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "size-limit": "^11.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitepress": "^1.6.4",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5",
    "@antfu/eslint-config": "^6.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "size-limit": [
    {
      "name": "UMD Bundle",
      "path": "dist/index.js",
      "limit": "45 KB"
    },
    {
      "name": "UMD Bundle (minified)",
      "path": "dist/index.min.js",
      "limit": "20 KB"
    },
    {
      "name": "ES Module",
      "path": "esm/index.js",
      "limit": "40 KB",
      "import": "{ createRouter }"
    },
    {
      "name": "CommonJS",
      "path": "cjs/index.js",
      "limit": "40 KB"
    }
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "peerDependenciesMeta": {}
}