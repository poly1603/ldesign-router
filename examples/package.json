{
  "name": "@ldesign/router-examples",
  "version": "1.0.0",
  "type": "module",
  "description": "Complete examples demonstrating all features of @ldesign/router",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint ."
  },
  "dependencies": {
    "@ldesign/router": "workspace:*",
    "@ldesign/device": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@ldesign/template": "workspace:*",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.0.0",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "typescript": "^5.6.0",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  }
}
