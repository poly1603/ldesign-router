{
  "name": "@ldesign/router-vue",
  "version": "1.0.0",
  "description": "Vue 3 router library with enhanced features, built on vue-router",
  "keywords": [
    "vue",
    "vue3",
    "router",
    "navigation",
    "spa",
    "typescript",
    "vue-router"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router/packages/vue#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router/packages/vue"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs"
    },
    "./adapter": {
      "types": "./esm/adapter/index.d.ts",
      "import": "./esm/adapter/index.js",
      "require": "./cjs/adapter/index.cjs"
    },
    "./adapter/*": {
      "types": "./esm/adapter/*.d.ts",
      "import": "./esm/adapter/*.js",
      "require": "./cjs/adapter/*.cjs"
    },
    "./components": {
      "types": "./esm/components/index.d.ts",
      "import": "./esm/components/index.js",
      "require": "./cjs/components/index.cjs"
    },
    "./components/*": {
      "types": "./esm/components/*.d.ts",
      "import": "./esm/components/*.js",
      "require": "./cjs/components/*.cjs"
    },
    "./composables": {
      "types": "./esm/composables/index.d.ts",
      "import": "./esm/composables/index.js",
      "require": "./cjs/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./cjs/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./cjs/plugins/*.cjs"
    },
    "./router": {
      "types": "./esm/router/index.d.ts",
      "import": "./esm/router/index.js",
      "require": "./cjs/router/index.cjs"
    },
    "./router/*": {
      "types": "./esm/router/*.d.ts",
      "import": "./esm/router/*.js",
      "require": "./cjs/router/*.cjs"
    },
    "./types": {
      "types": "./esm/types/index.d.ts",
      "import": "./esm/types/index.js",
      "require": "./cjs/types/index.cjs"
    },
    "./types/*": {
      "types": "./esm/types/*.d.ts",
      "import": "./esm/types/*.js",
      "require": "./cjs/types/*.cjs"
    },
    "./__tests__/*": {
      "types": "./esm/__tests__/*.d.ts",
      "import": "./esm/__tests__/*.js",
      "require": "./cjs/__tests__/*.cjs"
    },
    "./cjs/components/RouterBreadcrumb.vue.css": "./cjs/components/RouterBreadcrumb.vue.css",
    "./cjs/components/RouterLink.vue.css": "./cjs/components/RouterLink.vue.css",
    "./cjs/components/RouterTabs.vue.css": "./cjs/components/RouterTabs.vue.css",
    "./cjs/components/RouterView.vue.css": "./cjs/components/RouterView.vue.css",
    "./cjs/demo/Home.vue.css": "./cjs/demo/Home.vue.css",
    "./cjs/demo/pages/About.vue.css": "./cjs/demo/pages/About.vue.css",
    "./cjs/demo/pages/Home.vue.css": "./cjs/demo/pages/Home.vue.css",
    "./cjs/index.css": "./cjs/index.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index.min.css": "./dist/index.min.css"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "esm",
    "cjs",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "rimraf es lib dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ldesign/router-core": "workspace:*",
    "@ldesign/device": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "vue-router": "^4.2.5"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}