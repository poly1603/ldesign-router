{
  "name": "@ldesign/router-vue",
  "version": "1.0.0",
  "description": "Vue 3 router library with enhanced features, built on vue-router",
  "keywords": [
    "vue",
    "vue3",
    "router",
    "navigation",
    "spa",
    "typescript",
    "vue-router"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router/packages/vue#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router/packages/vue"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./components": {
      "types": "./es/components/index.d.ts",
      "import": "./es/components/index.js",
      "require": "./lib/components/index.cjs"
    },
    "./components/*": {
      "types": "./es/components/*.d.ts",
      "import": "./es/components/*.js",
      "require": "./lib/components/*.cjs"
    },
    "./composables": {
      "types": "./es/composables/index.d.ts",
      "import": "./es/composables/index.js",
      "require": "./lib/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./es/composables/*.d.ts",
      "import": "./es/composables/*.js",
      "require": "./lib/composables/*.cjs"
    },
    "./plugins": {
      "types": "./es/plugins/index.d.ts",
      "import": "./es/plugins/index.js",
      "require": "./lib/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./es/plugins/*.d.ts",
      "import": "./es/plugins/*.js",
      "require": "./lib/plugins/*.cjs"
    },
    "./router": {
      "types": "./es/router/index.d.ts",
      "import": "./es/router/index.js",
      "require": "./lib/router/index.cjs"
    },
    "./router/*": {
      "types": "./es/router/*.d.ts",
      "import": "./es/router/*.js",
      "require": "./lib/router/*.cjs"
    },
    "./__tests__/*": {
      "types": "./es/__tests__/*.d.ts",
      "import": "./es/__tests__/*.js",
      "require": "./lib/__tests__/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "rimraf es lib dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ldesign/router-core": "workspace:*",
    "@ldesign/device": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "vue-router": "^4.2.5"
  },
  "peerDependencies": {
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@ldesign/engine": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "jsdom": "^24.0.0",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}