{
  "name": "@ldesign/router-vue2",
  "version": "1.0.0",
  "description": "Vue 2 router library with enhanced features, built on vue-router 3.x",
  "keywords": [
    "vue",
    "vue2",
    "router",
    "navigation",
    "spa",
    "typescript",
    "vue-router"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "rimraf es lib dist"
  },
  "dependencies": {
    "@ldesign/router-core": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^2.7.0",
    "vue-router": "^3.5.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
