{
  "name": "@ldesign/router-angular",
  "version": "1.0.0",
  "description": "Angular router library with enhanced features, built on @angular/router",
  "keywords": [
    "angular",
    "router",
    "navigation",
    "spa",
    "typescript",
    "angular-router"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router/packages/angular#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router/packages/angular"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./services": {
      "types": "./es/services/index.d.ts",
      "import": "./es/services/index.js",
      "require": "./lib/services/index.cjs"
    },
    "./guards": {
      "types": "./es/guards/index.d.ts",
      "import": "./es/guards/index.js",
      "require": "./lib/guards/index.cjs"
    },
    "./directives": {
      "types": "./es/directives/index.d.ts",
      "import": "./es/directives/index.js",
      "require": "./lib/directives/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "rimraf es lib dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "dependencies": {
    "@ldesign/router-core": "workspace:*"
  },
  "peerDependencies": {
    "@angular/common": "^17.0.0 || ^18.0.0",
    "@angular/core": "^17.0.0 || ^18.0.0",
    "@angular/router": "^17.0.0 || ^18.0.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:../../../../tools/builder",
    "@angular/common": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.18.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.2",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

