{
  "name": "@ldesign/router-core",
  "version": "1.0.0",
  "description": "Framework-agnostic router core library for ldesign",
  "keywords": [
    "router",
    "core",
    "framework-agnostic",
    "typescript",
    "navigation"
  ],
  "author": "ldesign",
  "license": "MIT",
  "homepage": "https://github.com/ldesign/ldesign/tree/main/packages/router/packages/core#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/ldesign/ldesign.git",
    "directory": "packages/router/packages/core"
  },
  "bugs": {
    "url": "https://github.com/ldesign/ldesign/issues"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    },
    "./features": {
      "types": "./es/features/index.d.ts",
      "import": "./es/features/index.js",
      "require": "./lib/features/index.cjs"
    },
    "./features/*": {
      "types": "./es/features/*.d.ts",
      "import": "./es/features/*.js",
      "require": "./lib/features/*.cjs"
    },
    "./history": {
      "types": "./es/history/index.d.ts",
      "import": "./es/history/index.js",
      "require": "./lib/history/index.cjs"
    },
    "./history/*": {
      "types": "./es/history/*.d.ts",
      "import": "./es/history/*.js",
      "require": "./lib/history/*.cjs"
    },
    "./router": {
      "types": "./es/router/index.d.ts",
      "import": "./es/router/index.js",
      "require": "./lib/router/index.cjs"
    },
    "./router/*": {
      "types": "./es/router/*.d.ts",
      "import": "./es/router/*.js",
      "require": "./lib/router/*.cjs"
    },
    "./types": {
      "types": "./es/types/index.d.ts",
      "import": "./es/types/index.js",
      "require": "./lib/types/index.cjs"
    },
    "./types/*": {
      "types": "./es/types/*.d.ts",
      "import": "./es/types/*.js",
      "require": "./lib/types/*.cjs"
    },
    "./utils": {
      "types": "./es/utils/index.d.ts",
      "import": "./es/utils/index.js",
      "require": "./lib/utils/index.cjs"
    },
    "./utils/*": {
      "types": "./es/utils/*.d.ts",
      "import": "./es/utils/*.js",
      "require": "./lib/utils/*.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "es",
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build",
    "build:watch": "ldesign-builder build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "clean": "rimraf es lib dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "mitt": "^3.0.1",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "eslint": "^9.18.0",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "pnpm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}